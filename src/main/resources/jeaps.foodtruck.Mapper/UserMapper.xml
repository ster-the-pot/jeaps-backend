<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="jeaps.foodtruck.Token.UserMapper">
    <select id="findByUsername" resultType="jeaps.foodtruck.common.user.user">
      SELECT * FROM user
      where
      username=#{username}
    </select>
</mapper>